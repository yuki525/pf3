 <%= form_with(model: @user, local: true) do |f| %>
   <%= f.label :name, "名前" %>
   <%= f.text_field :name, class: "form-control" %>

   <%= f.label :email, "email" %>
   <%= f.text_field :email, class: "form-control" %>

   <%= f.label :password_digest, "password" %>
   <%= f.text_field :password_digest, class: "form-control" %>

   <%= f.submit "登録" %>

  <% end %>